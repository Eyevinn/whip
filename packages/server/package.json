{
  "name": "@eyevinn/whip-endpoint",
  "version": "1.2.1",
  "description": "WebRTC HTTP ingestion protocol endpoint",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project ./",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node --inspect ./dist/server.js\"",
    "server": "node ./dist/server.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'spec/**/*.ts'"
  },
  "author": "Eyevinn Technology <work@eyevinn.se>",
  "contributors": [
    "Jonas Birm√© <jonas.birme@eyevinn.se> (Eyevinn Technology AB)",
    "Benjamin Wallberg <benjamin.wallberg@eyevinn.se> (Eyevinn Technology AB)"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eyevinn/whip.git"
  },
  "devDependencies": {
    "@mapbox/node-pre-gyp": "^1.0.9",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.8.1",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@koush/wrtc": "^0.5.3",
    "cross-fetch": "^3.1.5",
    "fast-xml-parser": "^4.0.7",
    "fastify": "^3.27.4",
    "fastify-cors": "^6.0.3",
    "fluent-ffmpeg": "^2.1.2",
    "node-fetch": "^2.6.5",
    "sdp-transform": "^2.14.1",
    "uuid": "^8.3.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
